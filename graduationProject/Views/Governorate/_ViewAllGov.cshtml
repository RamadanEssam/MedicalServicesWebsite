@model IEnumerable<graduationProject.Models.Governorate>
@foreach (var item in Model)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Gov_Name)
        </td>
        <td class="text-center">
            <a onclick="showInPopup('@Url.Action("AddOrEdit","Governorate",new {id=item.Gov_Id},Context.Request.Scheme)','Edit')" class="btn btn-info text-white"><i class="fas fa-pencil-alt"></i>Edit</a>
            <form asp-action="Delete" asp-route-id="@item.Gov_Id" onsubmit="return jQueryAjaxDelete(this)" class="d-inline">
                <input type="hidden" asp-for="@item.Gov_Id" />
                 <input type="submit" value="Delete" class="btn btn-danger" />
            </form>
        </td>
    </tr>
}
